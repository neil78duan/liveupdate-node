{
  "port": 8008,
  "liveUpdateDir": "/home/liveupdate/upload",

  "openGMTool": "no",
  "gmtoolport": 8080,
  "gm_reliable_ip": [ "0.0.0.0" ],
  "my_domain_name": "http://localhost",

  "downloadCfg": {
    "desc": "about addition data download type 0 common,1 memecache ,2 fragment transfer . please do not change default",
    "type": 2,
    "fragment_size": 102400
  },

  "logger": {
    "desc": " enable yes/no, logtype log4/console/all , loglevel is debug, common ,warn, error ,fatal",
    "enable": "yes",
    "logtype": "log4",
    "loglevel": "error",
    "log4_config": {
      "appenders": [
        {
          "type": "file",
          "filename": "/var/log/liveupdate.log",
          "maxLogSize": "1048576",
          "backups": "10"
        }
      ]
    }

  }

}
